programa
{
	// funao F(x)
	
	funcao inicio()
	{
		inteiro  x=4, fx

		enquanto (x <=10) {
			fx = (2*x*x) + (3*x) -5
			escreva("\nO valor de Fx para x=",x," sera = ",fx)
			x = x + 1
		}

				
	}
}
